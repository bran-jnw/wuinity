<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Reminiscence</id>
    <version>1.3.0</version>
    <title>Reminiscence</title>
    <authors>Ben Abelshausen</authors>
